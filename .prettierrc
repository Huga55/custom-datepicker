{
  "useTabs": false,
  "printWidth": 80,
  "tabWidth": 2,
  "singleQuote": false,
  "trailingComma": "es5",
  "jsxBracketSameLine": false,
  "semi": true,
  "importOrder": [
    "^react$",
    "^[a-zA-Z].*$",
    "^@pages.*$",
    "^@services.*$",
    "^@schemas.*$",
    "^@components.*$",
    "^@templates.*$",
    "^@organisms.*$",
    "^@molecules.*$",
    "^@atoms.*$",
    "^@hooks.*$",
    "^@context.*$",
    "^@constants.*$",
    "^@images.*$",
    "^@svg.*$",
    "^@.*$",
    "^..?/.*$",
    "^.*[css, scss, sass]$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
